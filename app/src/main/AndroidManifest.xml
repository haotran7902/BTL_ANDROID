<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_shopee"
        android:label="Shopee"
        android:roundIcon="@mipmap/ic_shopee"
        android:supportsRtl="true"
        android:theme="@style/Theme.BTL_ANDROID"
        tools:targetApi="31">
        <activity
            android:name=".DetailOrderActivity"
            android:exported="false" />
        <activity
            android:name=".AddVoucherActivity"
            android:exported="false" />
        <activity
            android:name=".AddOrderActivity"
            android:exported="false" />
        <activity
            android:name=".UpdateDeleteProductActivity"
            android:exported="false" />
        <activity
            android:name=".DetailProductActivity"
            android:exported="false" />
        <activity
            android:name=".AddProductActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".MyReceiver"
            android:exported="false"/>
    </application>

</manifest>